
{
  "manifest_version": 2,
  "name": "PhishMeNot",
  "version": "1.0",
  "permissions": ["webNavigation"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "192": "images/icon_192.png"
    }
  }
}
